<p id="notice"><%= notice %></p>

<h1>Reservacions</h1>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Espacio comun</th>
      <th>Fecha</th>
      <th>T inicio</th>
      <th>T fin</th>
      <th>Estatus reserva</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reservacions.each do |reservacion| %>
      <tr>
        <td><%= reservacion.usuario_id %></td>
        <td><%= reservacion.espacio_comun_id %></td>
        <td><%= reservacion.fecha %></td>
        <td><%= reservacion.t_inicio %></td>
        <td><%= reservacion.t_fin %></td>
        <td><%= reservacion.estatus_reserva_id %></td>
        <td><%= link_to 'Show', reservacion %></td>
        <td><%= link_to 'Edit', edit_reservacion_path(reservacion) %></td>
        <td><%= link_to 'Destroy', reservacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reservacion', new_reservacion_path %>
